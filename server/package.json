{
  "name": "minifymy.link",
  "version": "1.0.0",
  "description": "A URL shortener",
  "main": "server.js",
  "engines": {
    "node": ">= 6.13.0"
  },
  "configurations": [
    {
      "name": "node: server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "doppler",
      "runtimeArgs": ["run", "--", "npm","start"]
    }
  ],
  "scripts": {
    "server-dev": "NODE_ENV=development npx nodemon ./server.js --trace-warnings",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "www.github.com/laurendayle/minifymy.link"
  },
  "keywords": [
    "url",
    "shortener",
    "shorten"
  ],
  "author": "Lauren Anderson",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "mongodb": "^4.8.0",
    "mongoose": "^6.4.6",
    "morgan": "^1.10.0",
    "nanoid": "3.3.4",
    "nodemon": "^2.0.19",
    "valid-url": "^1.0.9"
  }
}
